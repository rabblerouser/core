  "name": "@rabblerouser/core-frontend",
  "version": "0.1.0",
  "description": "Frontend for rabblerouser - A pluggable, extensible membership database for community organising",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "yarn webpack --progress --colors",
    "ci": "yarn lint && yarn test-single",
    "lint": "yarn eslint --ext js .",
    "start": "yarn webpack-dev-server --hot --inline --content-base public",
    "test": "yarn karma start",
    "test-single": "yarn karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/rabblerouser/core.git"
  },
  "bugs": {
    "url": "https://github.com/rabblerouser/core/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
