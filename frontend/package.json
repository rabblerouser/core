    "react-modal": "^1.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redux": "^3.5.2",
    "redux-form": "^6.0.0-rc.5",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "styled-components": "^1.2.1",
    "svg-url-loader": "^1.0.3",
    "validator": "^5.2.0",
    "webpack": "^1.13.0"
